{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "@agiflowai/clawdbot-mcp-plugin",
  "projectType": "library",
  "sourceRoot": "packages/clawdbot-mcp-plugin/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": [
        "{projectRoot}/dist"
      ],
      "options": {
        "command": "tsdown",
        "cwd": "packages/clawdbot-mcp-plugin"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "packages/clawdbot-mcp-plugin"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "biome check packages/clawdbot-mcp-plugin"
      }
    },
    "lint:fix": {
      "executor": "nx:run-commands",
      "options": {
        "command": "biome check --write packages/clawdbot-mcp-plugin"
      }
    }
  },
  "tags": [
    "type:library"
  ],
  "sourceTemplate": "typescript-lib"
}
