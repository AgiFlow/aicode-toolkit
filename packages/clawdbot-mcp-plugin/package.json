{
  "name": "@agiflowai/clawdbot-mcp-plugin",
  "description": "Clawdbot plugin for MCP server integration with progressive tool disclosure",
  "version": "0.1.1",
  "license": "AGPL-3.0",
  "author": "AgiflowIO",
  "repository": {
    "type": "git",
    "url": "https://github.com/AgiFlow/aicode-toolkit.git",
    "directory": "packages/clawdbot-mcp-plugin"
  },
  "homepage": "https://github.com/AgiFlow/aicode-toolkit#readme",
  "bugs": {
    "url": "https://github.com/AgiFlow/aicode-toolkit/issues"
  },
  "keywords": [
    "typescript",
    "library",
    "clawdbot",
    "mcp",
    "plugin"
  ],
  "clawdbot": {
    "extensions": [
      "dist/plugin.js"
    ]
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "files": [
    "dist",
    "README.md",
    "clawdbot.plugin.json"
  ],
  "scripts": {
    "build": "tsdown",
    "test": "vitest --run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@agiflowai/one-mcp": "workspace:*",
    "@sinclair/typebox": "^0.34.0"
  },
  "devDependencies": {
    "@modelcontextprotocol/sdk": "1.24.0",
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^3.0.0",
    "tsdown": "^0.15.6",
    "typescript": "5.9.3",
    "vitest": "4.0.15"
  },
  "peerDependencies": {
    "clawdbot": ">=0.1.0"
  },
  "peerDependenciesMeta": {
    "clawdbot": {
      "optional": true
    }
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./plugin": {
      "import": "./dist/plugin.js",
      "require": "./dist/plugin.cjs"
    },
    "./package.json": "./package.json"
  }
}
